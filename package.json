{
  "name": "nodecg-parcel-ts-preact",
  "version": "0.0.1",
  "description": "A nodecg example using Parcel2 + TypeScript + Preact",
  "repository": "git@github.com:nt3rp/nodecg-parcel-ts-preact.git",
  "author": "Nick Terwoord <nicholas.terwoord+code@gmail.com>",
  "license": "MIT",
  "scripts": {
    "watch:graphics": "parcel watch 'src/graphics/*.html' --dist-dir='graphics' --public-url='.'",
    "build:graphics": "parcel build 'src/graphics/*.html' --dist-dir='graphics' --public-url='.'"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  },
  "devDependencies": {
    "@parcel/optimizer-htmlnano": "2.0.0-nightly.617",
    "@parcel/packager-html": "2.0.0-nightly.617",
    "@parcel/transformer-html": "2.0.0-nightly.617",
    "@parcel/transformer-posthtml": "2.0.0-nightly.617",
    "@parcel/transformer-typescript-tsc": "^2.0.0-nightly.617",
    "@parcel/validator-typescript": "^2.0.0-nightly.617",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "parcel": "^2.0.0-nightly.615",
    "preact": "^10.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.0"
  }
}
